#include <iostream>
#include <vector>
#include <cstdlib>

using namespace std;

int partition(vector<int>& arr, int low, int high) {
    int pivot = arr[high];
    int i = low;
    for (int j = low; j < high; j++) {
        if (arr[j] <= pivot) {
            swap(arr[i], arr[j]);
            i++;
        }
    }
    swap(arr[i], arr[high]);
    return i;
}

int quickSelect(vector<int>& arr, int low, int high, int k) {
    if (k > 0 && k <= high - low + 1) {
        int pos = partition(arr, low, high);
        if (pos - low == k - 1) return arr[pos];
        if (pos - low > k - 1) return quickSelect(arr, low, pos - 1, k);
        return quickSelect(arr, pos + 1, high, k - (pos - low + 1));
    }
    return -1;
}

int main() {
    int t;
    cin >> t;
    while (t--) {
        int n, k;
        cin >> n;
        vector<int> arr(n);
        for (int i = 0; i < n; i++) cin >> arr[i];
        cin >> k;

        if (k > n || k <= 0) {
            cout << "not present" << endl;
        } else {
            cout << quickSelect(arr, 0, n - 1, k) << endl;
        }
    }
    return 0;
}